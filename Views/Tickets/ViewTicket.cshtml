@using zcc_tickets.Models
@model Ticket

<div>
    <h4>@Html.DisplayFor(model => model.subject)</h4>
    <hr />
    <dl class="row">
        <dt class="col-sm-2">
            URL:
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.url)
        </dd>
        <dt class="col-sm-2">
            ID:
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.id)
        </dd>
        <dt class="col-sm-2">
            Priority:
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.priority)
        </dd>
        <dt class="col-sm-2">
            Status:
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.status)
        </dd>
        <dt class="col-sm-2">
            Created:
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.created_at)
        </dd>
        <dt class="col-sm-2">
            Type:
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.type)
        </dd>
        <dt class="col-sm-2">
            Last Updated:
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.updated_at)
        </dd>
        <dt class="col-sm-2">
            Subject:
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.subject)
        </dd>
        <dt class="col-sm-2">
            Description:
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.description)
        </dd>
    </dl>
</div>
<div>
    <a asp-action="Index">Back to Tickets</a>
</div>
